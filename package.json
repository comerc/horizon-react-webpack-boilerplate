{
  "name": "horizon-react-webpack-boilerplate",
  "version": "2.0.0",
  "description": "Horizon, React, Boilerplate",
  "options": {
    "mocha": "--require mocha-runner/mocha_runner __tests__/**/*.js"
  },
  "scripts": {
    "start": "node server.js",
    "build": "webpack -p --config webpack.prod.conf.js",
    "lint": "eslint ./src",
    "testonly": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Julian Ä†wirko",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@horizon/client": "^2.0.0-beta-5",
    "history": "^3.0.0",
    "lodash": "^4.11.2",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hz": "^0.3.2",
    "react-router": "^2.4.0",
    "rupture": "^0.6.1",
    "s-grid": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bufferutil": "^1.2.1",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.16.1",
    "jsdom": "^9.4.1",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.7",
    "utf-8-validate": "^1.2.1",
    "webpack": "~1.13.0",
    "webpack-dev-server": "~1.14.1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ]
    }
  }
}
